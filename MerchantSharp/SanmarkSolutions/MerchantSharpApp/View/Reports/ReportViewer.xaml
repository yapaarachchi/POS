<Window x:Class="MerchantSharp.SanmarkSolutions.MerchantSharpApp.View.Reports.ReportViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:rv="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
        Title="ReportViewer" Height="280" Width="670" SizeToContent="Manual" WindowStartupLocation="CenterScreen" WindowState="Maximized">
    <Grid>
        <WindowsFormsHost x:Name="wfhReport">
            <rv:ReportViewer x:Name="rvReport" Load="rvReport_Load" />
        </WindowsFormsHost>
    </Grid>
</Window>
